                            "$ref": "#/definitions/LLMModelType"
                        }
                    ]
                },
                "max_tokens": {
                    "title": "Max Tokens",
                    "description": "The maximum number of tokens to generate in the completion",
                    "exclusiveMinimum": 0,
                    "type": "integer"
                },
                "temperature": {
                    "title": "Temperature",
                    "description": "What sampling temperature to use, between 0 and 2",
                    "default": 1.0,
                    "exclusiveMinimum": 0.0,
                    "maximum": 2.0,
                    "type": "number"
                }
            },
            "definitions": {
                "OutputTypeType": {
                    "title": "OutputTypeType",
                    "description": "Output type for the completion result",
                    "enum": [
                        "string",
                        "file"
                    ],
                    "type": "string"
                },
                "LLMModelType": {
                    "title": "LLMModelType",
                    "description": "The name of the model to use.\nOptions: ada, babbage, curie, davinci",
                    "enum": [
                        "text-ada-001",
                        "text-babbage-001",
                        "text-curie-001",
                        "text-davinci-003"
                    ],
                    "type": "string"
                }
            }
        },
        "output_schema": {
            "title": "OutputModel",
            "description": "Completions Piece output model",
            "type": "object",
            "properties": {
                "message": {
                    "title": "Message",
                    "description": "Output message to log",
                    "type": "string"
                },
                "string_completion_result": {
                    "title": "String Completion Result",
                    "description": "The result of the completion as a string",
                    "type": "string"
                },
                "file_completion_result": {
                    "title": "File Completion Result",
                    "description": "The result of the completion as a .txt file",
                    "format": "file-path",
                    "type": "string"
                },
                "usage_total_tokens": {
                    "title": "Usage Total Tokens",
                    "description": "The total number of tokens used in the completion",
                    "type": "integer"
                }
            },
            "required": [
                "message",
                "usage_total_tokens"
            ]
        },
